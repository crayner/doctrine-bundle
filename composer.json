{
    "name": "crayner/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine Bundle for Symfony 4+",
    "keywords": [
        "doctrine"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Rayner",
            "email": "craig@craigrayner.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2"
    },
    "require-dev": {
        "symfony/flex": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Crayner\\Doctrine\\": "/src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Crayner\\Doctrine\\Tests\\": "tests/"
        }
    }
}
